{
  "name": "weerd/vulcan-crud",
  "description": "Laravel package for creating CRUD related files.",
  "keywords": ["crud", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Diego Lorenzo",
      "email": "diego@diegolorenzo.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Weerd\\VulcanCrud\\": "src/"
    }
  },
  "require": {
    "illuminate/console": "^5.5@dev"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Weerd\\VulcanCrudTests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "symfony/var-dumper": "^3.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Weerd\\VulcanCrud\\VulcanCrudServiceProvider"
      ]
    }
  }
}
